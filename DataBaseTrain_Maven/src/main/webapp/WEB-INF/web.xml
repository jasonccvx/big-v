<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>servlet</servlet-name>
    <servlet-class>Servlet.autoRun</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>servlet2</servlet-name>
    <servlet-class>Servlet.autoRun2</servlet-class>
    <load-on-startup>-1</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>servlet3</servlet-name>
    <servlet-class>Servlet.autoRun3</servlet-class>
    <load-on-startup>-1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>servlet</servlet-name>
    <url-pattern>/autoRun</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>servlet2</servlet-name>
    <url-pattern>/autoRun2</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>servlet3</servlet-name>
    <url-pattern>/autoRun3</url-pattern>
  </servlet-mapping>
</web-app>



